// AÃ±o bisiesto Diana Guevara 181864
 
#include <stdio.h>
int main() 
{
    int ano;
    printf("Introduzca un ano:\n");
    scanf("%d", &ano);
    if ((ano % 4 == 0 && ano % 100 != 0)|| (ano % 400 == 0))
	{
        printf("%d es un ano bisiesto\n", ano);
    } 
	else 
	{
        printf("%d no es un ano bisiesto\n", ano);
    }
    return 0;
}
