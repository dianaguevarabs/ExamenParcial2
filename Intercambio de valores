// Intercambio de valores Diana Guevara 181864

#include <stdio.h>
int main() 
{
    int v, v1, v2, v3;
    printf ("Valor de v1:");
    scanf ("%d", &v1);
    printf ("Valor de v2:");
    scanf ("%d", &v2);
    printf ("Valor de v3:");
    scanf ("%d", &v3);
    v = v1;
    v1 = v2;
    v2 = v3;
    v3 = v;
    printf ("\n");
    printf ("Intercambio de valores:\n");
    printf ("v1:%d\n", v1);
    printf ("v2:%d\n", v2);
    printf ("v3:%d\n", v3);
    return 0;
}
